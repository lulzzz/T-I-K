{
  "processService.server.serviceName": "ProcessService-Server",
  "processService.server.serviceDisplayName": "ProcessService Server",
  "processService.server.serviceDescription": "web service for business service.",
  "processService.server.serviceAddress": "http://localhost:9005/",
  "processService.server.website": "http://localhost:9005/",

  "processService.authen.issuerSigningKey": "fiver-secret-key",
  "processService.authen.validIssuer": "Fiver.Security.Bearer",
  "processService.authen.validAudience": "Fiver.Security.Bearer",
  "processService.authen.expiryInMinutes": "1",

  "processService.authen.claimMembershipId": "MembershipId",
  "processService.authen.claimTokenId": "jti",
  "processService.authen.claimUserName": "http://schemas.xmlsoap.org/ws/2005/05/identity/claims/nameidentifier",
  "processService.authen.claimExpire": "exp",

  "processService.user": "admin",
  "processService.pwd": "abc@123",
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "ServiceDiscovery": {
    "ServiceName": "Online",
    "HealthCheckTemplate": "/HealthCheck",
    "Endpoints": [ "http://127.0.0.1:5300" ],
    "Consul": {
      "HttpEndpoint": "http://127.0.0.1:8500",
      "DnsEndpoint": {
        "Address": "127.0.0.1",
        "Port": 8600
      }
    }
  }
}
